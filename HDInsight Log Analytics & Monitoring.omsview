{
    "$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
        "location": {
            "type": "string",
            "defaultValue": ""
        },
        "resourcegroup": {
            "type": "string",
            "defaultValue": ""
        },
        "subscriptionId": {
            "type": "string",
            "defaultValue": ""
        },
        "workspace": {
            "type": "string",
            "defaultValue": ""
        },
        "workspaceapiversion": {
            "type": "string",
            "defaultValue": ""
        }
    },
    "resources": [
        {
            "apiVersion": "[parameters('workspaceapiversion')]",
            "name": "[parameters('workspace')]",
            "type": "Microsoft.OperationalInsights/workspaces",
            "location": "[parameters('location')]",
            "id": "[Concat('/subscriptions/', parameters('subscriptionId'), '/resourceGroups/', parameters('resourcegroup'), '/providers/Microsoft.OperationalInsights/workspaces/', parameters('workspace'))]",
            "resources": [
                {
                    "apiVersion": "2015-11-01-preview",
                    "name": "ceb3f134-2f7a-4314-bf58-a1fe0527c004",
                    "type": "views",
                    "location": "[parameters('location')]",
                    "id": "[Concat('/subscriptions/', parameters('subscriptionId'), '/resourceGroups/', parameters('resourcegroup'), '/providers/Microsoft.OperationalInsights/workspaces/', parameters('workspace'),'/views/ceb3f134-2f7a-4314-bf58-a1fe0527c004')]",
                    "dependson": [
                        "[Concat('/subscriptions/', parameters('subscriptionId'), '/resourceGroups/', parameters('resourcegroup'), '/providers/Microsoft.OperationalInsights/workspaces/', parameters('workspace'))]"
                    ],
                    "properties": {
                        "Id": "ceb3f134-2f7a-4314-bf58-a1fe0527c004",
                        "Name": "HDInsight Log Analytics & Monitoring",
                        "Description": "Gain Hadoop Operational Intelligence in HDinsight",
                        "Author": "ashishth@microsoft.com",
                        "Source": "Local",
                        "Dashboard": [
                            {
                                "Id": "InformationBlade",
                                "Type": "Blade",
                                "Version": 0,
                                "Configuration": {
                                    "General": {
                                        "Title": "",
                                        "NewGroup": false,
                                        "Color": "#00bcf2"
                                    },
                                    "Header": {
                                        "Image": "data:image/png;base64,",
                                        "Label": "Log Analytics, Monitoring & Alerting ",
                                        "Link": {
                                            "Label": "Learn More about HDInsight",
                                            "Url": "https://azure.microsoft.com/en-us/services/hdinsight/"
                                        }
                                    },
                                    "List": [
                                        {
                                            "Title": "A managed Apache Hadoop, Spark, R, HBase, and Storm cloud service made easy",
                                            "Content": "HDInsight solution provides Log Analytics, Monitoring and Alerting capabilities for following HDInsight cluster types. \n\nHadoop \n\nHBase \n\nSpark \n\nStorm \n\nR-Server \n\n \n\n\n"
                                        }
                                    ]
                                }
                            },
                            {
                                "Id": "LineChartBuilderBlade",
                                "Type": "Blade",
                                "Version": 0,
                                "Configuration": {
                                    "General": {
                                        "title": "",
                                        "newGroup": false,
                                        "icon": "",
                                        "useIcon": false
                                    },
                                    "Header": {
                                        "Title": "Process Time ",
                                        "Subtitle": ""
                                    },
                                    "LineChart": {
                                        "Query": "Type:Perf CounterName=\"% Processor Time\" InstanceName=\"_Total\" Computer='hn*-*' or Computer='zk*-*' or Computer='wn*-*' | measure avg(CounterValue) by Computer ",
                                        "yAxis": {
                                            "isLogarithmic": false,
                                            "units": {
                                                "baseUnitType": "Percentage",
                                                "baseUnit": "ZeroToOne",
                                                "displayUnit": "ZeroToOne"
                                            },
                                            "customLabel": ""
                                        }
                                    },
                                    "List": {
                                        "Query": "Type:Perf CounterName=\"% Processor Time\" InstanceName=\"_Total\" Computer='hn*-*' or Computer='zk*-*' or Computer='wn*-*' | measure avg(CounterValue) by Computer ",
                                        "HideGraph": false,
                                        "enableSparklines": false,
                                        "operation": "Summary",
                                        "ColumnsTitle": {
                                            "Name": "CPU Processor Time",
                                            "Value": "Value"
                                        },
                                        "Color": "#00bcf2",
                                        "thresholds": {
                                            "isEnabled": false,
                                            "values": [
                                                {
                                                    "name": "Normal",
                                                    "threshold": "Default",
                                                    "color": "#009e49",
                                                    "isDefault": true
                                                },
                                                {
                                                    "name": "Warning",
                                                    "threshold": 60,
                                                    "color": "#fcd116"
                                                },
                                                {
                                                    "name": "Error",
                                                    "threshold": 90,
                                                    "color": "#ba141a"
                                                }
                                            ]
                                        },
                                        "NameDSVSeparator": "",
                                        "NavigationQuery": "{selected item}"
                                    }
                                }
                            },
                            {
                                "Id": "NotableQueriesBuilderBlade",
                                "Type": "Blade",
                                "Version": 0,
                                "Configuration": {
                                    "General": {
                                        "title": "Hadoop Log Search",
                                        "newGroup": false,
                                        "preselectedFilters": "",
                                        "renderMode": "grid"
                                    },
                                    "queries": [
                                        {
                                            "query": "Type=regionserverlogs_CL",
                                            "displayName": "HBaseRegion Server Logs"
                                        },
                                        {
                                            "query": "Type=zookeeperlogs_CL ",
                                            "displayName": "HBase Zookeeper  Logs"
                                        },
                                        {
                                            "query": "Type=restserverlog_CL",
                                            "displayName": "HBase Rest Server Logs"
                                        },
                                        {
                                            "query": "Type=log_yarnresourcemanagerlog_CL",
                                            "displayName": "Yarn Resource Manager Logs"
                                        },
                                        {
                                            "query": "Type=log_hadoopmapreducejobsummarylog_CL",
                                            "displayName": "Map-Reduce Job Summery Logs "
                                        }
                                    ]
                                }
                            },
                            {
                                "Id": "LineChartSparklineBuilderBlade",
                                "Type": "Blade",
                                "Version": 0,
                                "Configuration": {
                                    "General": {
                                        "title": "",
                                        "newGroup": false,
                                        "icon": "",
                                        "useIcon": false
                                    },
                                    "Header": {
                                        "Title": "HBase Region Count",
                                        "Subtitle": ""
                                    },
                                    "lineChart": {
                                        "query": "Type= metrics_regionserver_CL | measure avg(regionCount_d) by tagHostname_s ",
                                        "yAxis": {
                                            "isLogarithmic": false,
                                            "units": {
                                                "baseUnitType": "",
                                                "baseUnit": "",
                                                "displayUnit": ""
                                            },
                                            "customLabel": ""
                                        }
                                    },
                                    "List": {
                                        "Query": "Type= metrics_regionserver_CL | measure avg(regionCount_d) by tagHostname_s ",
                                        "operation": "Last Sample",
                                        "HideGraph": false,
                                        "enableSparklines": true,
                                        "ColumnsTitle": {
                                            "Name": "Worker Node",
                                            "Value": "Regions"
                                        },
                                        "Color": "#0072c6",
                                        "thresholds": {
                                            "isEnabled": false,
                                            "values": [
                                                {
                                                    "name": "Normal",
                                                    "threshold": "Default",
                                                    "color": "#009e49",
                                                    "isDefault": true
                                                },
                                                {
                                                    "name": "Warning",
                                                    "threshold": 60,
                                                    "color": "#fcd116"
                                                },
                                                {
                                                    "name": "Error",
                                                    "threshold": 90,
                                                    "color": "#ba141a"
                                                }
                                            ]
                                        },
                                        "NavigationQuery": "{selected item}"
                                    }
                                }
                            },
                            {
                                "Id": "LineChartSparklineBuilderBlade",
                                "Type": "Blade",
                                "Version": 0,
                                "Configuration": {
                                    "General": {
                                        "title": "",
                                        "newGroup": false,
                                        "icon": "",
                                        "useIcon": false
                                    },
                                    "Header": {
                                        "Title": "HBase Read & Writes",
                                        "Subtitle": ""
                                    },
                                    "lineChart": {
                                        "query": "Type= metrics_regionserver_CL | measure avg(readRequestCount_d), avg(writeRequestCount_d) by tagHostname_s ",
                                        "yAxis": {
                                            "isLogarithmic": false,
                                            "units": {
                                                "baseUnitType": "",
                                                "baseUnit": "",
                                                "displayUnit": ""
                                            },
                                            "customLabel": ""
                                        }
                                    },
                                    "List": {
                                        "Query": "Type= metrics_regionserver_CL | measure avg(readRequestCount_d), avg(writeRequestCount_d) by tagHostname_s interval 1minute",
                                        "operation": "Summary",
                                        "HideGraph": false,
                                        "enableSparklines": true,
                                        "ColumnsTitle": {
                                            "Name": "Node",
                                            "Value": "Average"
                                        },
                                        "Color": "#0072c6",
                                        "thresholds": {
                                            "isEnabled": false,
                                            "values": [
                                                {
                                                    "name": "Normal",
                                                    "threshold": "Default",
                                                    "color": "#009e49",
                                                    "isDefault": true
                                                },
                                                {
                                                    "name": "Warning",
                                                    "threshold": 60,
                                                    "color": "#fcd116"
                                                },
                                                {
                                                    "name": "Error",
                                                    "threshold": 90,
                                                    "color": "#ba141a"
                                                }
                                            ]
                                        },
                                        "NavigationQuery": "{selected item}"
                                    }
                                }
                            },
                            {
                                "Id": "LineChartSparklineBuilderBlade",
                                "Type": "Blade",
                                "Version": 0,
                                "Configuration": {
                                    "General": {
                                        "title": "",
                                        "newGroup": false,
                                        "icon": "",
                                        "useIcon": false
                                    },
                                    "Header": {
                                        "Title": "HBase hLog file count",
                                        "Subtitle": ""
                                    },
                                    "lineChart": {
                                        "query": "Type= metrics_regionserver_CL | measure avg(hlogFileCount_d) by tagHostname_s ",
                                        "yAxis": {
                                            "isLogarithmic": false,
                                            "units": {
                                                "baseUnitType": "",
                                                "baseUnit": "",
                                                "displayUnit": ""
                                            },
                                            "customLabel": ""
                                        }
                                    },
                                    "List": {
                                        "Query": "Type= metrics_regionserver_CL | measure avg(hlogFileCount_d) by tagHostname_s ",
                                        "operation": "Summary",
                                        "HideGraph": false,
                                        "enableSparklines": true,
                                        "ColumnsTitle": {
                                            "Name": "Type",
                                            "Value": "Count"
                                        },
                                        "Color": "#0072c6",
                                        "thresholds": {
                                            "isEnabled": false,
                                            "values": [
                                                {
                                                    "name": "Normal",
                                                    "threshold": "Default",
                                                    "color": "#009e49",
                                                    "isDefault": true
                                                },
                                                {
                                                    "name": "Warning",
                                                    "threshold": 60,
                                                    "color": "#fcd116"
                                                },
                                                {
                                                    "name": "Error",
                                                    "threshold": 90,
                                                    "color": "#ba141a"
                                                }
                                            ]
                                        },
                                        "NavigationQuery": "{selected item}"
                                    }
                                }
                            },
                            {
                                "Id": "NotableQueriesBuilderBlade",
                                "Type": "Blade",
                                "Version": 0,
                                "Configuration": {
                                    "General": {
                                        "title": "Popular HBase charts",
                                        "newGroup": false,
                                        "preselectedFilters": "Type, Computer",
                                        "renderMode": "grid"
                                    },
                                    "queries": [
                                        {
                                            "query": "Type=  metrics_regionserveros_CL | measure avg(OpenFileDescriptorCount_d), avg(MaxFileDescriptorCount_d) by ObjectName_s interval 1minute",
                                            "displayName": "Open Files vs Total Open Files"
                                        },
                                        {
                                            "query": "Type=  metrics_regionserveros_CL | measure avg(TotalPhysicalMemorySize_d), avg(FreePhysicalMemorySize_d) by ObjectName_s interval 1minute",
                                            "displayName": "Free vs Total Memory"
                                        },
                                        {
                                            "query": " Type=  metrics_regionserver_CL  | measure avg(CommittedVirtualMemorySize_d) by ObjectName_s interval 1minute",
                                            "displayName": "Committed Virtual Memory Size"
                                        },
                                        {
                                            "query": " Type=metrics_regionserveros_CL  | measure avg(SystemLoadAverage_d) by ObjectName_s interval 1minute",
                                            "displayName": "Average Load"
                                        },
                                        {
                                            "query": "Type=metrics_regionserveros_CL | measure avg(SystemCpuLoad_d) by ObjectName_s interval 1minute",
                                            "displayName": "CPU Load"
                                        }
                                    ]
                                }
                            }
                        ],
                        "OverviewTile": {
                            "Id": "LineChartBuilderTile",
                            "Type": "OverviewTile",
                            "Version": 0,
                            "Configuration": {
                                "LineChart": {
                                    "Query": "Type:Perf CounterName=\"% Processor Time\" InstanceName=\"_Total\" Computer='hn*-*' or Computer='zk*-*' or Computer='wn*-*' | measure avg(CounterValue) by Computer ",
                                    "yAxis": {
                                        "isLogarithmic": true,
                                        "units": {
                                            "baseUnitType": "Percentage",
                                            "baseUnit": "ZeroToOne",
                                            "displayUnit": "ZeroToOne"
                                        },
                                        "customLabel": ""
                                    }
                                },
                                "Advanced": {
                                    "DataFlowVerification": {
                                        "Enabled": true,
                                        "Query": "Type= metrics_regionserver_CL",
                                        "Message": "Performing Assessment - ensure that you have run the following [Script Action](https://raw.githubusercontent.com/duoxu/darfo/master/script.sh)"
                                    }
                                }
                            }
                        }
                    }
                }
            ]
        }
    ]
}